<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="org.example.pkgDTO.DTOEstudiantesEntity">
        <table name="estudiantes" schema="public" catalog="ProyectoFinalP2"/>
        <attributes>
            <id name="idEstudiante">
                <column name="id_estudiante"/>
            </id>
            <basic name="nombre">
                <column name="nombre"/>
            </basic>
            <basic name="apellido">
                <column name="apellido"/>
            </basic>
            <basic name="email">
                <column name="email"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
